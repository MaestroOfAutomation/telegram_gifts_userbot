# Common Telegram API credentials for all accounts
API_ID=12345
API_HASH=abcdef1234567890abcdef1234567890

# Maximum supply threshold - will try to buy gifts when supply is <= this number
SUPPLY_THRESHOLD=10

# Check interval in milliseconds (500ms = 0.5 seconds)
CHECK_INTERVAL_MS=500

# Telegram accounts (comma-separated strings)
# Format: phone_number:target_channel_id
# Example: +1234567890:-100123123123,+0987654321:-100456456456
TELEGRAM_ACCOUNTS=

# Telegram notification bot token
TELEGRAM_BOT_TOKEN=1231231231:AAFj1jijasdfisjdfisjdifjsd

# Telegram notification channel IDs
# These are the chat IDs where notifications will be sent
TELEGRAM_WARNING_CHANNEL_ID=-4116110111
TELEGRAM_SUCCESS_CHANNEL_ID=-4111103118
TELEGRAM_ERROR_CHANNEL_ID=-4116119115

# Telegram controller bot token (for sending stickers and handling inline buttons)
TELEGRAM_CONTROLLER_BOT_TOKEN=1231231231:AAFj1jijasdfisjdfisjdifjsd

# Telegram controller channel ID
# This is the chat ID where gift stickers and information will be sent
TELEGRAM_CONTROLLER_CHANNEL_ID=-4116110111

# Maximum number of gifts to buy per client account
MAX_GIFTS_TO_BUY=5

# Test gift ID (optional)
# If specified, this gift ID will be included in the filter even if it's already in the cache
# Set to "none" or leave empty to disable
# You can use 5870720080265871962 (sold out gift) for testing
TEST_GIFT_ID=